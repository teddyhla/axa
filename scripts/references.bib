	author = {Hasegawa, Takanori and Yamaguchi, Rui and Kakuta, Masanori and Sawada, Kaori and Kawatani, Kenichi and Murashita, Koichi and Nakaji, Shigeyuki and Imoto, Seiya},
	urldate = {2022-02-24},
	date = {2020-03-20},
	langid = {english},
	note = {Publisher: Public Library of Science},
	keywords = {Blood, Cholesterol, Cohort studies, Diabetes mellitus, Dyslipidemia, Electronic medical records, Genetics, Social status},
	file = {Full Text PDF:/Users/tunwinhla/Zotero/storage/FCQG6TA8/Hasegawa et al. - 2020 - Prediction of blood test values under different li.pdf:application/pdf},
}

@article{yu_predict_2020,
	title = {Predict or draw blood: An integrated method to reduce lab tests},
