@article{grasselli_hospital-acquired_2021,
	title = {Hospital-Acquired Infections in Critically Ill Patients With {COVID}-19},
	volume = {0},
	issn = {0012-3692},
	url = {https://journal.chestnet.org/article/S0012-3692(21)00679-6/abstract},
	doi = {10.1016/j.chest.2021.04.002},
	abstract = {{\textless}h3{\textgreater}Background{\textless}/h3{\textgreater}{\textless}p{\textgreater}Few small studies have described hospital-acquired infections ({HAIs}) occurring in patients with {COVID}-19.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Research Question{\textless}/h3{\textgreater}{\textless}p{\textgreater}What characteristics in critically ill patients with {COVID}-19 are associated with {HAIs} and how are {HAIs} associated with outcomes in these patients?{\textless}/p{\textgreater}{\textless}h3{\textgreater}Study Design and Methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}Multicenter retrospective analysis of prospectively collected data including adult patients with severe {COVID}-19 admitted to eight Italian hub hospitals from February 20, 2020, through May 20, 2020. Descriptive statistics and univariate and multivariate Weibull regression models were used to assess incidence, microbial cause, resistance patterns, risk factors (ie, demographics, comorbidities, exposure to medication), and impact on outcomes (ie, {ICU} discharge, length of {ICU} and hospital stays, and duration of mechanical ventilation) of microbiologically confirmed {HAIs}.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}Of the 774 included patients, 359 patients (46\%) demonstrated 759 {HAIs} (44.7 infections/1,000 {ICU} patient-days; 35\% multidrug-resistant [{MDR}] bacteria). Ventilator-associated pneumonia ({VAP}; n = 389 [50\%]), bloodstream infections ({BSIs}; n = 183 [34\%]), and catheter-related {BSIs} (n = 74 [10\%]) were the most frequent {HAIs}, with 26.0 (95\% {CI}, 23.6-28.8) {VAPs} per 1,000 patient intubation-days, 11.7 (95\% {CI}, 10.1-13.5) {BSIs} per 1,000 {ICU} patient-days, and 4.7 (95\% {CI}, 3.8-5.9) catheter-related {BSIs} per 1,000 {ICU} patient-days. Gram-negative bacteria (especially \textit{Enterobacterales}) and \textit{Staphylococcus aureus} caused 64\% and 28\% of cases of {VAP}, respectively. Variables independently associated with infection were age, positive end expiratory pressure, and treatment with broad-spectrum antibiotics at admission. Two hundred thirty-four patients (30\%) died in the {ICU} (15.3 deaths/1,000 {ICU} patient-days). Patients with {HAIs} complicated by septic shock showed an almost doubled mortality rate (52\% vs 29\%), whereas noncomplicated infections did not affect mortality. {HAIs} prolonged mechanical ventilation (median, 24 days [interquartile range ({IQR}), 14-39 days] vs 9 days [{IQR}, 5-13 days]; \textit{P} {\textless} .001), {ICU} stay (24 days [{IQR}, 16-41 days] vs 9 days [{IQR}, 6-14 days]; \textit{P} = .003), and hospital stay (42 days [{IQR}, 25-59 days] vs 23 days [{IQR}, 13-34 days]; \textit{P} {\textless} .001).{\textless}/p{\textgreater}{\textless}h3{\textgreater}Interpretation{\textless}/h3{\textgreater}{\textless}p{\textgreater}Critically ill patients with {COVID}-19 are at high risk for {HAIs}, especially {VAPs} and {BSIs} resulting from {MDR} organisms. {HAIs} prolong mechanical ventilation and hospitalization, and {HAIs} complicated by septic shock almost doubled mortality.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Trial Registry{\textless}/h3{\textgreater}{\textless}p{\textgreater}{ClinicalTrials}.gov; No.: {NCT}04388670; {URL}: www.clinicaltrials.gov{\textless}/p{\textgreater}},
	number = {0},
	journaltitle = {{CHEST}},
	shortjournal = {{CHEST}},
	author = {Grasselli, Giacomo and Scaravilli, Vittorio and Mangioni, Davide and Scudeller, Luigia and Alagna, Laura and Bartoletti, Michele and Bellani, Giacomo and Biagioni, Emanuela and Bonfanti, Paolo and Bottino, Nicola and Coloretti, Irene and Cutuli, Salvatore Lucio and Pascale, Gennaro De and Ferlicca, Daniela and Fior, Gabriele and Forastieri, Andrea and Franzetti, Marco and Greco, Massimiliano and Guzzardella, Amedeo and Linguadoca, Sara and Meschiari, Marianna and Messina, Antonio and Monti, Gianpaola and Morelli, Paola and Muscatello, Antonio and Redaelli, Simone and Stefanini, Flavia and Tonetti, Tommaso and Antonelli, Massimo and Cecconi, Maurizio and Foti, Giuseppe and Fumagalli, Roberto and Girardis, Massimo and Ranieri, Marco and Viale, Pierluigi and Raviglione, Mario and Pesenti, Antonio and Gori, Andrea and Bandera, Alessandra},
