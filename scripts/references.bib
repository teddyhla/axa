	number = {4},
	journaltitle = {British Journal of Anaesthesia},
	shortjournal = {Br J Anaesth},
	author = {Eugene, N. and Oliver, C. M. and Bassett, M. G. and Poulton, T. E. and Kuryba, A. and Johnston, C. and Anderson, I. D. and Moonesinghe, S. R. and Grocott, M. P. and Murray, D. M. and Cromwell, D. A. and Walker, K. and {NELA collaboration}},
	date = {2018-10},
