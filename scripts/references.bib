	pmcid = {PMC8939225},
	keywords = {Humans, Intensive Care Units, Acute Kidney Injury, Kidney, Time-to-Treatment, Acute kidney injury, Heterogeneity of treatment effect, Personalized medicine, Renal replacement therapy, Renal Replacement Therapy},
