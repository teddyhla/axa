	keywords = {Humans, Female, Male, Middle Aged, Randomized Controlled Trials as Topic, Aged, Retrospective Studies, Adult, Machine Learning, Biomarkers, Respiratory Distress Syndrome, Inflammation Mediators, Phenotype, Predictive Value of Tests},
	file = {Full Text:/Users/tunwinhla/Zotero/storage/QCUVZFY3/Sinha et al. - 2020 - Development and validation of parsimonious algorit.pdf:application/pdf},
}

