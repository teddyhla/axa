	number = {3},
	journaltitle = {Journal of the American Medical Informatics Association},
	shortjournal = {Journal of the American Medical Informatics Association},
	author = {Cai, Xiongcai and Perez-Concha, Oscar and Coiera, Enrico and Martin-Sanchez, Fernando and Day, Richard and Roffe, David and Gallego, Blanca},
	urldate = {2021-06-24},
	date = {2016-05-01},
	keywords = {prediction},
