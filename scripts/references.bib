	pmid = {30028362},
	keywords = {Humans, Female, Male, Middle Aged, Aged, Retrospective Studies, Adult, Intensive Care Units, Sepsis, Critical Illness, Cohort Studies, Treatment Outcome, Follow-Up Studies, Lactic Acid, Protective Agents, Thiamine, Vitamin B Complex},
