	number = {1},
	journaltitle = {{CPT}: pharmacometrics \& systems pharmacology},
	shortjournal = {{CPT} Pharmacometrics Syst Pharmacol},
	author = {Asiimwe, Innocent G. and Blockman, Marc and Cohen, Karen and Cupido, Clint and Hutchinson, Claire and Jacobson, Barry and Lamorde, Mohammed and Morgan, Jennie and Mouton, Johannes P. and Nakagaayi, Doreen and Okello, Emmy and Schapkaitz, Elise and Sekaggya-Wiltshire, Christine and Semakula, Jerome R. and Waitt, Catriona and Zhang, Eunice J. and Jorgensen, Andrea L. and Pirmohamed, Munir},
