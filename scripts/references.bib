	number = {5},
	journaltitle = {European Heart Journal. Cardiovascular Pharmacotherapy},
	shortjournal = {Eur Heart J Cardiovasc Pharmacother},
	author = {Goto, Shinichi and Goto, Shinya and Pieper, Karen S. and Bassand, Jean-Pierre and Camm, Alan John and Fitzmaurice, David A. and Goldhaber, Samuel Z. and Haas, Sylvia and Parkhomenko, Alexander and Oto, Ali and Misselwitz, Frank and Turpie, Alexander G. G. and Verheugt, Freek W. A. and Fox, Keith A. A. and Gersh, Bernard J. and Kakkar, Ajay K.},
