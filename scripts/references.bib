	note = {Publisher: British Medical Journal Publishing Group
Section: Intensive care},
	keywords = {adult intensive \& critical care, physiology, respiratory medicine (see thoracic medicine), mv},
	file = {Full Text PDF:/Users/tunwinhla/Zotero/storage/NSH2SVXP/Vizcaychipi et al. - 2020 - Intensive Care Weaning (iCareWean) protocol on wea.pdf:application/pdf;Snapshot:/Users/tunwinhla/Zotero/storage/I4U4EAQ6/e042145.html:text/html},
