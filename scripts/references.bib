	keywords = {Humans, Female, Male, Middle Aged, Aged, Case-Control Studies, Odds Ratio, Cohort Studies, Anticoagulants, Hemorrhage, International Normalized Ratio, Models, Statistical, Administration, Oral, Thrombosis, Heart Valve Prosthesis},
