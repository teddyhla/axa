	number = {1},
	journaltitle = {Critical Care (London, England)},
	shortjournal = {Crit Care},
	author = {Grolleau, François and Porcher, Raphaël and Barbar, Saber and Hajage, David and Bourredjem, Abderrahmane and Quenot, Jean-Pierre and Dreyfuss, Didier and Gaudry, Stéphane},
	date = {2022-03-21},
