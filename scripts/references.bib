	pmcid = {PMC8176336},
	keywords = {Humans, machine learning, Retrospective Studies, Anticoagulants, Treatment Outcome, clinical decision support, dosage recommendation, Heparin, intensive care unit, Models, Statistical, outcome prediction, Prognosis},
