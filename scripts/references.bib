	number = {1},
	journaltitle = {Cell Discovery},
	shortjournal = {Cell Discov},
	author = {Zhong, Huanzi and Wang, Yanqun and Shi, Zhun and Zhang, Lu and Ren, Huahui and He, Weiqun and Zhang, Zhaoyong and Zhu, Airu and Zhao, Jingxian and Xiao, Fei and Yang, Fangming and Liang, Tianzhu and Ye, Feng and Zhong, Bei and Ruan, Shicong and Gan, Mian and Zhu, Jiahui and Li, Fang and Li, Fuqiang and Wang, Daxi and Li, Jiandong and Ren, Peidi and Zhu, Shida and Yang, Huanming and Wang, Jian and Kristiansen, Karsten and Tun, Hein Min and Chen, Weijun and Zhong, Nanshan and Xu, Xun and Li, Yi-min and Li, Junhua and Zhao, Jincun},
	urldate = {2021-06-10},
