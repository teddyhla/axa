	pmcid = {PMC2219864},
	file = {PubMed Central Full Text PDF:/Users/tunwinhla/Zotero/storage/4Y8Z6F2H/van Walraven and Forster - 2007 - Anticoagulation Control in the Peri-Hospitalizatio.pdf:application/pdf},
}

