	urldate = {2022-03-15},
	date = {2020-02-01},
	langid = {english},
	keywords = {quality improvement, warfarin, hemodialysis, time in therapeutic range},
	file = {ScienceDirect Full Text PDF:/Users/tunwinhla/Zotero/storage/9AJLUTE7/Blum et al. - 2020 - Quality Improvement Program Improves Time in Thera.pdf:application/pdf;ScienceDirect Snapshot:/Users/tunwinhla/Zotero/storage/UNP4F6AX/S2468024919315268.html:text/html},
