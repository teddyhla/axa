	urldate = {2022-09-19},
	date = {2021-03-31},
	langid = {english},
	file = {Bos et al. - 2021 - Precision medicine in acute respiratory distress s.pdf:/Users/tunwinhla/Zotero/storage/EU363GT2/Bos et al. - 2021 - Precision medicine in acute respiratory distress s.pdf:application/pdf},
