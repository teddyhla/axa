	author = {Delmas, Clément and Jacquemin, Aemilia and Vardon-Bounes, Fanny and Georges, Bernard and Guerrero, Felipe and Hernandez, Nicolas and Marcheix, Bertrand and Seguin, Thierry and Minville, Vincent and Conil, Jean-Marie and Silva, Stein},
