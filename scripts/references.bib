	keywords = {Critical Care, Humans, Risk Factors, Female, Male, Middle Aged, Age Factors, Aged, Hypertension, Adult, Atrial Fibrillation, Atrial fibrillation, Biomarkers, C-Reactive Protein, {ICU}, Incidence, Inflammation, Intensive Care Units, Odds Ratio, Prospective Studies, Risk factors, Sepsis, Systemic Inflammatory Response Syndrome},
